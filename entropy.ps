$av=@();foreach($a in $l=$(Get-Content ./test.pass)) {$licz=$a.ToCharArray() | Group-Object | foreach{ $p=$_.count / $a.Length ;$i=[Math]::Log($p, 2);-$p*$i} | measure -Sum ; $av+=([math]::Ceiling($licz.sum)*$a.Length); write-host "String:"$a "("$a.Length")" "`nEntropy:"$licz.sum"bits`nStrength:"([math]::Ceiling($licz.sum)*$a.Length) "bits`n================"} ;$sum=$av | measure -Minimum -Maximum -Average -Sum;Write-Host  "Samples: " $sum.Count`n"Max bits:" $sum.Maximum`n"Min bits:" $sum.Minimum`n"Average Strength bits:" $([math]::round($sum.Average)) -BackgroundColor red -ForegroundColor yellow
